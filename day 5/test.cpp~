#include <iostream>
#include <vector>
using namespace std;

int main () {
    int a;
    cin >> a;

    vector<int> vec;

    for (int i = 0; i < a ; i++) {

        int b, c;
        cin >> b;
        cin >> c;
        if( b > c) {
            vec.push_back(c);
        } else {
            vec.push_back(b);
        }
    }

    for (int j = 0; j < vec.size(); j++) {
        cout << vec[j];
    }
}

